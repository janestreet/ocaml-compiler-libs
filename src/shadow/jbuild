(library
 ((name        ocaml_shadow)
  (public_name ocaml-compiler-libs.shadow)
(flags ((:standard -safe-string) -w -49))
  (preprocess  no_preprocessing)
))

(rule
 ((targets (ocaml_shadow.ml))
  (deps    (gen/gen.exe))
  (action  (run ${<} -ocaml-where ${ocaml_where} -o ${@}))))

(jbuild_version 1)
